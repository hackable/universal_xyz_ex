{
  "name": "orderbook-ws-server",
  "version": "1.0.0",
  "description": "WebSocket server for order discovery in the Orderbook smart contract",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
